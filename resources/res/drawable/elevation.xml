<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:state_enabled="true" android:state_pressed="true">
        <set>
            <objectAnimator android:duration="@android:integer/config_shortAnimTime" android:valueFrom="2dp" android:valueTo="4dp" android:valueType="floatType" android:propertyName="translationZ"/>
        </set>
    </item>
    <item>
        <set>
            <objectAnimator android:duration="@android:integer/config_shortAnimTime" android:valueFrom="4dp" android:valueTo="2dp" android:valueType="floatType" android:propertyName="translationZ"/>
        </set>
    </item>
</selector>
