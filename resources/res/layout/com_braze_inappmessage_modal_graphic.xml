<?xml version="1.0" encoding="utf-8"?>
<com.braze.p055ui.inappmessage.views.InAppMessageModalView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:id="@+id/com_braze_inappmessage_modal_container" android:padding="0dp" android:layout_width="match_parent" android:layout_height="match_parent">
