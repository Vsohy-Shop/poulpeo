            <LinearLayout android:gravity="center" android:layout_gravity="center" android:id="@+id/com_braze_inappmessage_modal_button_layout_single" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" style="@style/Braze.InAppMessage.Layout.Button.Modal">
