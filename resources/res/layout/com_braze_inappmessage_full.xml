<?xml version="1.0" encoding="utf-8"?>
<com.braze.p055ui.inappmessage.views.InAppMessageFullView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:padding="0dp" android:layout_width="match_parent" android:layout_height="match_parent">
    <RelativeLayout android:id="@+id/com_braze_inappmessage_full_frame" style="@style/Braze.InAppMessage.Frame.Full"/>
    <RelativeLayout android:id="@+id/com_braze_inappmessage_full" style="@style/Braze.InAppMessage.Full">
        <LinearLayout android:orientation="vertical" android:id="@+id/com_braze_inappmessage_full_all_content_parent" android:padding="0dp" android:layout_width="match_parent" android:layout_height="match_parent">
            <com.braze.p055ui.inappmessage.views.InAppMessageImageView android:id="@+id/com_braze_inappmessage_full_imageview" style="@style/Braze.InAppMessage.Image.Full"/>
            <LinearLayout android:id="@+id/com_braze_inappmessage_full_text_and_button_content_parent" android:padding="0dp" style="@style/Braze.InAppMessage.Full.TextAndButtonContent">
                <ScrollView android:id="@+id/com_braze_inappmessage_full_scrollview" style="@style/Braze.InAppMessage.ScrollView.Full">
                    <LinearLayout android:id="@+id/com_braze_inappmessage_full_text_layout" android:layout_height="wrap_content" style="@style/Braze.InAppMessage.Full.TextArea">
                        <TextView android:id="@+id/com_braze_inappmessage_full_header_text" style="@style/Braze.InAppMessage.Header.Full"/>
                        <TextView android:id="@+id/com_braze_inappmessage_full_message" style="@style/Braze.InAppMessage.Message.Full"/>
                    </LinearLayout>
                </ScrollView>
                <LinearLayout android:gravity="center" android:layout_gravity="center" android:id="@+id/com_braze_inappmessage_full_button_layout_single" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" style="@style/Braze.InAppMessage.Layout.Button.Full">
                    <com.braze.p055ui.inappmessage.views.InAppMessageButton android:id="@+id/com_braze_inappmessage_full_button_single_one" style="@style/Braze.InAppMessage.Button.Full.Single"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/com_braze_inappmessage_full_button_layout_dual" android:visibility="gone" style="@style/Braze.InAppMessage.Layout.Button.Full">
                    <LinearLayout android:gravity="center" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
                        <com.braze.p055ui.inappmessage.views.InAppMessageButton android:id="@+id/com_braze_inappmessage_full_button_dual_one" style="@style/Braze.InAppMessage.Button.Full.Dual.One"/>
                    </LinearLayout>
                    <LinearLayout android:gravity="center" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
                        <com.braze.p055ui.inappmessage.views.InAppMessageButton android:id="@+id/com_braze_inappmessage_full_button_dual_two" style="@style/Braze.InAppMessage.Button.Full.Dual.Two"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <RelativeLayout android:layout_width="match_parent" android:layout_height="match_parent">
            <ImageButton android:id="@+id/com_braze_inappmessage_full_close_button" style="@style/Braze.InAppMessage.CloseButton.Full"/>
        </RelativeLayout>
    </RelativeLayout>
</com.braze.p055ui.inappmessage.views.InAppMessageFullView>
